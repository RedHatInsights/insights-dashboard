{
  "name": "insights-dashboard",
  "version": "1.0.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/RedHatInsights/insights-dashboard"
  },
  "license": "Apache-2.0",
  "author": "Red Hat",
  "scripts": {
    "analyze": "NODE_ENV=production webpack --config config/dev.webpack.config.js --env.prod=true --env.analyze=true --mode production",
    "build": "webpack --config config/prod.webpack.config.js",
    "lint": "npm-run-all lint:*",
    "lint-fix": "eslint config src --fix",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "nightly": "npm run travis:verify",
    "prod": "NODE_ENV=production webpack-dev-server --config config/dev.webpack.config.js",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "NODE_ENV=development webpack-dev-server --config config/dev.webpack.config.js",
    "test": "jest --verbose",
    "translations:compile": "npm run build; node node_modules/@redhat-cloud-services/frontend-components-utilities/files/mergeMessages.js",
    "travis:build": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "travis:verify": "npm-run-all travis:build lint test",
    "verify": "npm-run-all build lint test"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js|jsx}",
      "!src/**/stories/*"
    ],
    "coverageDirectory": "./coverage/",
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "roots": [
      "<rootDir>/src/"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ]
  },
  "dependencies": {
    "@babel/runtime": "7.9.2",
    "@patternfly/react-charts": "5.3.18",
    "@patternfly/react-core": "3.153.3",
    "@patternfly/react-icons": "3.15.15",
    "@patternfly/react-table": "2.28.29",
    "@patternfly/react-tokens": "2.8.12",
    "@red-hat-insights/insights-frontend-components": "3.41.70",
    "@redhat-cloud-services/frontend-components": "1.0.19",
    "@redhat-cloud-services/frontend-components-notifications": "1.0.2",
    "@redhat-cloud-services/frontend-components-translations": "0.0.6",
    "@redhat-cloud-services/frontend-components-utilities": "1.0.1",
    "classnames": "2.2.6",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-intl": "4.4.0",
    "react-redux": "5.0.7",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-promise-middleware": "6.1.2",
    "seamless-immutable": "7.1.4",
    "urijs": "1.19.2"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-object-rest-spread": "7.9.5",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@babel/preset-flow": "7.9.0",
    "@babel/preset-react": "7.9.4",
    "@redhat-cloud-services/frontend-components-config": "1.0.0",
    "axios": "0.19.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.3.0",
    "babel-plugin-dual-import": "1.2.1",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-react-intl": "7.1.0",
    "babel-polyfill": "6.26.0",
    "css-loader": "3.5.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-loader": "3.0.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "3.0.0",
    "file-loader": "6.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "25.3.0",
    "moment": "2.24.0",
    "npm-run-all": "4.1.5",
    "prop-types": "15.7.2",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.4",
    "stylelint": "13.3.2",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-scss": "3.17.0",
    "webpack": "4.42.1",
    "webpack-bundle-analyzer": "3.7.0",
    "webpack-cli": "3.3.11"
  },
  "insights": {
    "appname": "dashboard"
  }
}
