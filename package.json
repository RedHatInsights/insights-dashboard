{
  "name": "insights-dashboard",
  "version": "1.0.0",
  "private": false,
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=11.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RedHatInsights/insights-dashboard"
  },
  "license": "Apache-2.0",
  "author": "Red Hat",
  "scripts": {
    "build": "fec build",
    "lint": "npm-run-all lint:*",
    "lint-fix": "eslint config src --fix",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "nightly": "npm run ci:verify",
    "postinstall": "ts-patch install",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "fec static",
    "start:proxy": "PROXY=true fec dev",
    "test": "jest --passWithNoTests",
    "test:coverage": "curl -sSL 'https://raw.githubusercontent.com/RedHatInsights/insights-interact-tools/refs/heads/main/scripts/coverage.sh' | bash",
    "test:ct": "BABEL_ENV=componentTest cypress run --browser chrome --component",
    "test:openct": "cypress open --component",
    "translations": "npm-run-all translations:*",
    "translations:extract": "npx formatjs extract ./src/*.js --out-file ./build/messages/src/Messages.json",
    "translations:compile": "npx formatjs compile ./build/messages/src/Messages.json --out-file ./locales/translations.json",
    "ci:verify": "npm run test:coverage",
    "verify": "npm-run-all build lint test"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!src/**/*.cy.js"
    ],
    "coverageDirectory": "coverage-jest",
    "moduleNameMapper": {
      "\\.(css|less|scss|sass|svg|png)$": "identity-obj-proxy"
    },
    "roots": [
      "<rootDir>/src/"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "transformIgnorePatterns": [],
    "testEnvironment": "jsdom"
  },
  "dependencies": {
    "@babel/runtime": "^7.26.7",
    "@patternfly/patternfly": "^6.3.1",
    "@patternfly/react-charts": "^8.3.1",
    "@patternfly/react-core": "^6.3.1",
    "@patternfly/react-icons": "^6.3.1",
    "@patternfly/react-table": "^6.3.1",
    "@patternfly/react-tokens": "^6.3.1",
    "@redhat-cloud-services/frontend-components": "^7.0.3",
    "@redhat-cloud-services/frontend-components-notifications": "^6.1.5",
    "@redhat-cloud-services/frontend-components-translations": "^4.0.10",
    "@redhat-cloud-services/frontend-components-utilities": "^7.0.3",
    "@sentry/webpack-plugin": "^3.1.0",
    "@unleash/proxy-client-react": "^4.1.2",
    "classnames": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.6.1",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.21.3",
    "redux": "^5.0.1",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "redux-promise-middleware": "^6.2.0",
    "seamless-immutable": "^7.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@babel/eslint-parser": "^7.26.5",
    "@babel/eslint-plugin": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-react": "^7.26.3",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@cypress/code-coverage": "^3.12.37",
    "@formatjs/cli": "^4.2.33",
    "@redhat-cloud-services/frontend-components-config": "^6.6.11",
    "@redhat-cloud-services/tsc-transform-imports": "^1.0.23",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.2",
    "@testing-library/user-event": "^14.5.2",
    "axios": "^1.8.4",
    "babel-jest": "^29.7.0",
    "babel-plugin-istanbul": "^7.0.0",
    "css-loader": "^6.8.1",
    "cypress": "^14.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "9.1.2",
    "eslint-plugin-cypress": "^2.15.2",
    "eslint-plugin-jest-dom": "^5.2.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-testing-library": "^6.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "qs": "^6.10.3",
    "react-masonry-css": "^1.0.16",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-scss": "^3.20.1",
    "ts-patch": "^3.1.2",
    "typescript": "^5.3.3"
  },
  "insights": {
    "appname": "dashboard"
  },
  "nyc": {
    "report-dir": "coverage-cypress",
    "include": [
      "src/**/*.{js,tsx}"
    ],
    "exclude": [
      "src/**/*.test.js",
      "src/**/*.cy.js"
    ]
  }
}
