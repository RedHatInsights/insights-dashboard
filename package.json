{
  "name": "insights-dashboard",
  "version": "1.0.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/RedHatInsights/insights-dashboard"
  },
  "license": "Apache-2.0",
  "author": "Red Hat",
  "scripts": {
    "analyze": "NODE_ENV=production webpack --config config/dev.webpack.config.js --env prod=true --env analyze=true --mode production",
    "build": "webpack --config config/prod.webpack.config.js",
    "lint": "npm-run-all lint:*",
    "lint-fix": "eslint config src --fix",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "nightly": "npm run travis:verify",
    "prod": "NODE_ENV=production webpack serve --config config/dev.webpack.config.js",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "NODE_ENV=development webpack serve --config config/dev.webpack.config.js",
    "test": "jest --verbose",
    "translations": "npm-run-all translations:*",
    "translations:extract": "npx formatjs extract ./src/*.js --out-file ./build/messages/src/Messages.json",
    "translations:compile": "npx formatjs compile ./build/messages/src/Messages.json --out-file ./locales/translations.json",
    "travis:build": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "travis:verify": "npm-run-all travis:build lint test",
    "verify": "npm-run-all build lint test"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js|jsx}",
      "!src/**/stories/*"
    ],
    "coverageDirectory": "./coverage/",
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "roots": [
      "<rootDir>/src/"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(@patternfly/react-tokens/|@patternfly/react-icons/)).*$"
    ]
  },
  "dependencies": {
    "@babel/runtime": "7.12.5",
    "@patternfly/patternfly": "4.80.3",
    "@patternfly/react-charts": "6.13.8",
    "@patternfly/react-core": "4.90.2",
    "@patternfly/react-icons": "4.8.4",
    "@patternfly/react-table": "4.20.15",
    "@patternfly/react-tokens": "4.9.26",
    "@redhat-cloud-services/frontend-components": "3.0.1",
    "@redhat-cloud-services/frontend-components-charts": "^3.0.0",
    "@redhat-cloud-services/frontend-components-translations": "^3.0.0",
    "@redhat-cloud-services/frontend-components-utilities": "3.0.2",
    "classnames": "2.2.6",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-intl": "5.10.19",
    "react-redux": "7.2.2",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-promise-middleware": "6.1.2",
    "seamless-immutable": "7.1.4"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/eslint-plugin": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "7.12.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-flow": "7.12.1",
    "@babel/preset-react": "7.12.10",
    "@formatjs/cli": "3.0.5",
    "@redhat-cloud-services/frontend-components-config": "3.0.0-beta17",
    "axios": "0.21.1",
    "babel-jest": "26.6.3",
    "babel-plugin-dual-import": "1.2.1",
    "babel-plugin-transform-imports": "^2.0.0",
    "babel-polyfill": "6.26.0",
    "css-loader": "5.0.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "6.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "moment": "2.29.1",
    "npm-run-all": "4.1.5",
    "prop-types": "15.7.2",
    "qs": "6.9.6",
    "sass-loader": "10.1.1",
    "style-loader": "2.0.0",
    "stylelint": "13.9.0",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-scss": "3.18.0",
    "typescript": "4.1.3",
    "webpack": "5.18.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "4.4.0"
  },
  "insights": {
    "appname": "dashboard"
  },
  "routes": {
    "dashboard": "/dashboard"
  }
}
